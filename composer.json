{
    "name": "javanile/webhook",
    "description": "Automate your deploy with Webhook",
    "version": "0.0.30",
    "license": "MIT",
    "type": "application",
    "authors": [
        {
            "name": "Francesco Bianco",
            "email": "bianco@javanile.org"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/javanile/webhook"
        }
    ],
    "autoload": {
        "psr-4": {
            "Javanile\\Webhook\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javanile\\Webhook\\Tests\\": "tests/"
        }
    },
    "require": {
        "javanile/urlman": "^0.0.5",
        "yalesov/cron-expr-parser": "^2.0",
        "squizlabs/php_codesniffer": "*",
        "danielstjules/stringy": "^3.1",
        "guzzlehttp/guzzle": "^6.3",
        "monolog/monolog": "^1.23"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "javanile/producer": "^0.0.18"
    }
}